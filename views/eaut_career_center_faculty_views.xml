<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Form view -->
    <record id="eaut_career_center_faculty_view_form" model="ir.ui.view">
        <field name="name">eaut.career.center.faculty.view.form</field>
        <field name="model">eaut.career.center.faculty</field>
        <field name="arch" type="xml">
            <form string="Faculty">
                <sheet>
                    <group>
                        <field name="code"/>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- List view -->
    <record id="eaut_career_center_faculty_view_list" model="ir.ui.view">
        <field name="name">eaut.career.center.faculty.view.list</field>
        <field name="model">eaut.career.center.faculty</field>
        <field name="arch" type="xml">
            <list string="Facultys">
                <field name="name"/> 
                <field name="code"/>
            </list>
        </field>
    </record>

    <!-- Actions -->
    <record id="action_eaut_career_center_faculty" model="ir.actions.act_window">
        <field name="name">Faculty</field>
        <field name="res_model">eaut.career.center.faculty</field>
        <field name="view_mode">list,form,kanban</field>
    </record>

</odoo>